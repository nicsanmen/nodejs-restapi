CREATE DATABASE IF NOT EXISTS db;

use db;

CREATE TABLE user(
	id INT(11) NOT NULL AUTO_INCREMENT,
	username VARCHAR(20),
	email VARCHAR(40) DEFAULT NULL,
    PRIMARY KEY (id)
)

